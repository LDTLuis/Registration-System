-- Criar tabela usuarios:

CREATE TABLE usuarios (
login VARCHAR(50) PRIMARY KEY,
senha VARCHAR(50) NOT NULL,
tipo VARCHAR(10) NOT NULL,
matricula VARCHAR(20),
FOREIGN KEY (matricula) REFERENCES alunos(matricula)
);

